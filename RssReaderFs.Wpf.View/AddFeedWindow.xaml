<Window x:Class="RssReaderFs.Wpf.View.AddFeedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add Feed" Height="156.247" Width="298.833"
        Visibility="{Binding Visibility}"
        Closing="Window_Closing"
        FontFamily="Yu Gothic">
    <Grid>
        <Label Content="Name" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top"/>
        <Label Content="URL" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.706,2.002"/>
        <TextBox Text="{Binding Name, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Height="23" Margin="58,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="224"/>
        <TextBox Text="{Binding Url}" HorizontalAlignment="Left" Height="23" Margin="58,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="224"/>
        <Button Content="Add" Command="{Binding AddCommand}" HorizontalAlignment="Left" Margin="207,92,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="textBlock" Text="{Binding Error}" HorizontalAlignment="Left" Margin="10,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="-1.037,0.123" Height="37" Width="191"/>
    </Grid>
</Window>
